{
    "name": "gulo-solutions/BigCommerceWordPress",
    "description": "Wrapper around BigCommerce API",
    "keywords": ["API", "BigCommerce"],
    "version": "1.0",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.2",
        "bigcommerce/api": "~3.0",
        "incenteev/composer-parameter-handler": "~2.0"

    },
    "require-dev": {
        "phpunit/phpunit": "~6.3"
    },
    "autoload": {
        "psr-0": {
            "BigCommerceWordPress\\": "/"
        }
    },
    "config": {
        "vendor-dir": "vendor/"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "extra": {
        "incenteev-parameters": {
            "file": "BigCommerceWordPress/parameters.yml",
            "dist-file": "BigCommerceWordPress/parameters.yml.dist",
             "keep-outdated": true
        }
    }
}
